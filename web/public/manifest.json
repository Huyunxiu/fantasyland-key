{
  "manifest_version": 2,
  "name": "FX.TECH",
  "version": "1.0.0",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "logo.png",
    "32": "logo.png",
    "128": "logo.png"
},
  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "FX.TECH"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "omnibox": { "keyword" : "fx" },
  "permissions": ["tabs"]
}
